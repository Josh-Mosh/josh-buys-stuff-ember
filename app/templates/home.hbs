{{#if session.isAuthenticated}}
  {{link-to "Manage Sets" "sets"}}
{{else}}
  {{link-to "Admin Login" "login"}}
{{/if}}

<div class="row w-100">
  <div class="col-lg-3">
    <div class="home-about-wrapper text-center">
      <img class="about-logo" alt="Josh Buys Stuff" src="assets/images/brand-round-blue.png">
    </div>
  </div>

  <div class="col-lg-9">
    <div class="row">
      {{#each sets as |set|}}
        <div class="col-lg-4 col-md-6 set-card-wrapper pl-lg-0">
          {{set-card set=set}}
        </div>
      {{/each}}
    </div>
  </div>
</div>
